<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
   <!--选中时效果--> 
   <item android:state_focused="true" 
        android:drawable="@drawable/base_edittext_selected" />
   <!--disable效果 windows焦点在前时  注意写法-->     
   <item android:state_window_focused="true" android:state_enabled="false"   
        android:drawable="@drawable/base_edittext_normal" />
   <!--默认时效果-->
   <item  android:drawable="@drawable/base_edittext_normal" /> 
</selector>